<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
    
    
    


     <bean id="address" class="com.java.org.Addresses">
     
        <constructor-arg value="Disney world"> </constructor-arg>
        <constructor-arg  value="NA"></constructor-arg>
        <constructor-arg value="France"></constructor-arg>
        <constructor-arg value="600100"></constructor-arg>
        <constructor-arg value="America"></constructor-arg>
     
     </bean>
     
     <bean id="customer" class="com.java.org.Customer">
     
     
      <constructor-arg value="1"> </constructor-arg>
        <constructor-arg  value="Groot"></constructor-arg>
        <constructor-arg  value="9874561230"></constructor-arg>
        <constructor-arg ref="address"></constructor-arg>
        
     
     
     
     </bean>
        
     






</beans>